#!/bin/bash

# Checks if argument provided, else uses stdin
set -- "${1:-$(</dev/stdin)}" "${@:2}"

zsh_prompt="$(whoami)@$(hostname):"
drop_prompt="$(hostname):0.0.0 zsh $(hostname)"

case $1 in
  *Firefox)
    #firefox
    echo 
    ;;
  urxvt)
    #terminal
    echo 
    ;;
  "$zsh_prompt"*)
    #terminal
    echo 
    ;;
  "$drop_prompt")
    #caret-square-down
    echo 
    ;;
  "Ultimaker Cura")
    # Cubes
    echo 
    ;;
  Telegram)
    echo 
    ;;
  *'Arduino 1'*)
    # microchip
    echo 
    ;;
  *.pdf)
    echo 
    ;;
  GParted)
    # hdd
    echo 
    ;;
  'emacs@'*)
    # code
    echo 
    ;;
  'vim '*)
    # code
    echo 
    ;;
  'GNU Image Manipulation Program')
    # palette
    echo 
    ;;
  *'– GIMP')
    # palette
    echo 
    ;;
  'Bluetooth Devices')
    # bluetooth
    echo 
    ;;
  'ranger')
    #folder
    echo 
    ;;
esac
